<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity3">

    <Button
        android:id="@+id/botonvolver2"
        android:layout_width="163dp"
        android:layout_height="66dp"
        android:layout_marginStart="34dp"
        android:layout_marginEnd="33dp"
        android:layout_marginBottom="32dp"
        android:backgroundTint="?attr/colorError"
        android:text="Volver"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/botonsalir"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/webView2" />

    <Button
        android:id="@+id/botonsalir"
        android:layout_width="143dp"
        android:layout_height="67dp"
        android:layout_marginEnd="36dp"
        android:backgroundTint="?attr/colorError"
        android:text="Salir"
        android:textSize="14sp"
        app:layout_constraintBottom_toBottomOf="@+id/botonvolver2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/botonvolver2"
        app:layout_constraintTop_toTopOf="@+id/botonvolver2"
        app:layout_constraintVertical_bias="0.0" />

    <WebView
        android:id="@+id/webView2"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginBottom="30dp"
        app:layout_constraintBottom_toTopOf="@+id/botonvolver2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

    </WebView>
</androidx.constraintlayout.widget.ConstraintLayout>